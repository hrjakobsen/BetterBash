#!/bin/tesh

func range(int start, int end) int[] {
    int[] result = []
    int count = start
    if (start < end) {
        while count <= end {
            result ::= count
            count += 1
        }
    } else {
        while count <= end {
             result ::= count
             count -= 1
         }
    }

    return result
}

func sizeInt(int[] input) int {
    int size = 0
    for x in input {
        size += 1
    }
    return size
}

func sizeStr(string[] input) int {
    int size = 0
    for x in input {
        size += 1
    }
    return size
}

func random(float max) int {
    return floor(rnd() * max)
}



int size = 10
string[] trees = [":herb:", ":tulip:", ":seedling:", ":seedling:", ":sunflower:", ":hibiscus:", ":rose:", ":tulip:", ":blossom:", ":cherry_blossom:"]
